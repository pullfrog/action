{
  "name": "@pullfrog/sandbox",
  "version": "0.0.1",
  "description": "Landlock-based process sandboxing for Pullfrog",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "*.node"
  ],
  "napi": {
    "name": "sandbox",
    "triples": {
      "defaults": false,
      "additional": [
        "x86_64-unknown-linux-gnu"
      ]
    }
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4"
  },
  "engines": {
    "node": ">= 18"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pullfrog/action.git"
  }
}

